  "main": "app/background.js",
  "scripts": {
    "dev": "nextron",
    "build": "cross-env NEXT_PUBLIC_ESLINT_DISABLE=true NODE_ENV=production nextron build",
    "build:mac": "cross-env NEXT_PUBLIC_ESLINT_DISABLE=true NODE_ENV=production nextron build --mac",
    "build:win": "cross-env NEXT_PUBLIC_ESLINT_DISABLE=true NODE_ENV=production nextron build --win",
    "postinstall": "electron-builder install-app-deps",
    "lint": "next lint",
    "test": "jest --config jest.config.ts",
    "test:watch": "jest --watch --config jest.config.ts",
    "electron-dev": "concurrently \"npm run dev\" \"npm run electron\"",
    "electron": "wait-on tcp:3000 && electron ."
  },
